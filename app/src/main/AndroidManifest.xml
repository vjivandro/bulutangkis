<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bulutangkis.learning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MateriActivity"
            android:label="Materi"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".VideoActivity"
            android:label="Video Tutorial"
            android:parentActivityName=".VideoActivity" />
        <activity
            android:name=".materi.PeraturanBulutangkis"
            android:label="Peraturan Bulutangkis"
            android:parentActivityName=".MateriActivity" />
        <activity
            android:name=".materi.PerwasitanBulutangkis"
            android:label="Perwasitan Bulutangkis"
            android:parentActivityName=".MateriActivity" />
        <activity
            android:name=".materi.SejarahBulutangkis"
            android:label="Sejarah Bulutangkis"
            android:parentActivityName=".MateriActivity" />
        <activity
            android:name=".materi.StrategiBulutangkis"
            android:label="Strategi dan Taktik Bulutangkis"
            android:parentActivityName=".MateriActivity" />
        <activity
            android:name=".materi.TeknikServiceBulutangkis"
            android:label="Teknik Servis dan Pukulan Bulutangkis"
            android:parentActivityName=".MateriActivity" />
        <activity
            android:name=".about.ProfileActivity"
            android:label="Profil Apliksi"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".kuis.KuisActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".kuis.PrepareKuisActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".about.IdentitasActivity"
            android:label="Identitas"
            android:parentActivityName=".about.ProfileActivity" />
        <activity
            android:name=".about.PendidikanActivity"
            android:label="Pendidikan"
            android:parentActivityName=".about.ProfileActivity" />
        <activity
            android:name=".about.RiwayatPekerjaanActivity"
            android:label="Riwayat Pekerjaan"
            android:parentActivityName=".about.ProfileActivity"/>
    </application>

</manifest>